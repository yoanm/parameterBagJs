{
  "extends": "google",
  "rules": {
    "indent": [2,4, { "SwitchCase": 1 }],
    "camelcase": ["error"],
    "max-len": [1, 121],
    "max-nested-callbacks": [1, 5],
    "valid-jsdoc": ["error", {
      "requireReturn": false,
      "requireReturnType": true,
      "requireReturnDescription": false,
      "requireParamDescription": false
    }]
  }
}